{
  "name": "nuxtpress-client",
  "version": "1.0.0",
  "description": "My phenomenal Nuxt.js project",
  "author": "Jamie Warburton",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "style": "yarn stylelint 'assets/scss/**/*.(s?(a|c)ss|vue)' 'components/**/*.(s?(a|c)ss|vue)' --syntax scss",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,vue,scss}": "stylelint --syntax=scss",
    "*.{js,vue}": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/style-resources": "^1.0.0",
    "axios": "^0.19.0",
    "inuitcss": "^6.0.0",
    "inuitcss-hex": "^1.0.7",
    "node-sass": "^4.12.0",
    "nuxt": "^2.0.0",
    "sass-loader": "^8.0.0",
    "vuejs-logger": "^1.5.4",
    "wpapi": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime-corejs3": "^7.6.3",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.3.0",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.2.1",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-backgrounds": "^5.2.5",
    "@storybook/addon-centered": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addon-notes": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/vue": "^5.2.5",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "core-js": "3",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.6.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.16.4",
    "sass-resources-loader": "^2.0.1",
    "stylelint": "^13.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.14.2",
    "vue-jest": "^4.0.0-0"
  }
}
